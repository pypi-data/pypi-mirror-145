{
  "$id": "http://numengo.org/draft-04/Ngo/Fl/NgoPnodePhase",
  "$schema": "http://numengo.org/draft-XX/defs-schema#/definitions/model/definitions/ComponentDefinition",
  "name": "NgoPnodePhase",
  "canonicalName": "Ngo.Fl.NgoPnodePhase",
  "description": "subphase of thermodynamic state",
  "extends": [
    "Ngo.Fl.NgoPnode"
  ],
  "initializeFromInputs": true,
  "variableGroups": [
    {
      "name": "Parameters",
      "variables": [
        {
          "$schema": "http://numengo.org/draft-XX/defs-schema#/definitions/variable/definitions/EnumerationVariable",
          "name": "phase",
          "description": "phase at port",
          "variability": "Parameter",
          "recompile": false,
          "defaultValue": "#Parameters.phase.OVERALL",
          "literals": [
            {
              "name": "OVERALL",
              "description": "overall",
              "numericalValue": 1
            },
            {
              "name": "GAS",
              "description": "gas",
              "numericalValue": 2
            },
            {
              "name": "LIQUID",
              "description": "liquid",
              "numericalValue": 3
            },
            {
              "name": "SOLID",
              "description": "solid",
              "numericalValue": 4
            }
          ]
        }
      ]
    },
    {
      "name": "Variables",
      "variables": [
        {
          "$schema": "http://numengo.org/draft-XX/defs-schema#/definitions/variable/definitions/ObjectVariable",
          "name": "overallPnode",
          "receivesValueFrom": "#overallPort.pnode",
          "causality": "Input",
          "isPlottable": false,
          "externalClass": "Ngo.Fl.NgoFlExternalClasses.NgoSim::NgoPnode"
        }
      ]
    }
  ],
  "functions": [
    {
      "name": "calcPnodeIndex",
      "description": "override method to calculate pnode index",
      "inputVariables": [
        "#Variables.overallPnode"
      ],
      "returnVariable": "Ngo.Fl.NgoPnode.Variables.pnode",
      "replaceable": true
    }
  ],
  "ports": [
    {
      "name": "overallPort",
      "portType": "Ngo.Fl.SinglePIn",
      "variables": [
        {
          "$schema": "http://numengo.org/draft-XX/defs-schema#/definitions/variable/definitions/ObjectVariable",
          "name": "pnode",
          "sendsValueTo": [
            "#Variables.overallPnode"
          ],
          "causality": "Input",
          "alias": "Reference",
          "externalClass": "Ngo.Fl.NgoFlExternalClasses.NgoSim::NgoPnode"
        }
      ]
    }
  ],
  "generationInfo": {
    "name": "GenerationInfo",
    "description": "Generation Info",
    "createAdditionalWrappersUsingIndex": true,
    "extraMethodsWrappable": [
      {
        "name": "isPhase",
        "description": "method to know if pnode is a subphase",
        "returnType": "Boolean",
        "virtual": true,
        "body": {
          "sourceCode": "return true;"
        }
      }
    ]
  }
}
